    "@typescript-eslint/parser": "^5.4.0",
    "dockerode": "^3.3.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "get-port": "^5.1.1",
    "jest": "^27.4.0",
    "prettier": "^2.5.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.86",
    "@types/uuid": "^8.3.3",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1038.0",
    "uuid": "^8.3.2"
  }
