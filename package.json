{
  "name": "aws-test-suite",
  "version": "1.0.0",
  "description": "AWS test suite for typescript",
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint {src,test}/**/*.ts",
    "lint:fix": "eslint --fix {src,test}/**/*.ts",
    "build": "npm install && npm run lint && npm run test && npm run dist",
    "test": "jest --reporters='default' --runInBand",
    "dist": "webpack",
    "testDeployCF": "rm -r ./dist && npm install && npm run dist && cd docker && ./buildStack.sh CF",
    "testDeployTF": "rm -r ./dist && npm install && npm run dist && cd docker && ./buildStack.sh TF",
    "coverage": "jest --coverage"
  },
  "author": "Calvin Matthews",
  "license": "MIT",
  "devDependencies": {
    "@types/dockerode": "^3.3.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "dockerode": "^3.3.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "get-port": "^5.1.1",
    "jest": "^27.4.0",
    "prettier": "^2.5.0",
    "terser-webpack-plugin": "^5.2.5",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "zip-webpack-plugin": "^4.0.1"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.117",
    "@types/uuid": "^8.3.3",
    "aws-lambda": "^1.0.7",
    "@aws-sdk/client-cloudformation": "^3.350.0",
    "@aws-sdk/client-cloudwatch": "^3.350.0",
    "@aws-sdk/client-dynamodb": "^3.350.0",
    "@aws-sdk/client-s3": "^3.350.0",
    "@aws-sdk/client-ssm": "^3.350.0",
    "@aws-sdk/client-secrets-manager": "^3.350.0",
    "@aws-sdk/client-sqs": "^3.350.0",
    "@aws-sdk/util-dynamodb": "^3.350.0",
    "@aws-sdk/signature-v4-crt": "^3.347.0",
    "uuid": "^8.3.2"
  }
}
